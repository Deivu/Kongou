{
  "name": "kongou",
  "version": "4.0.0",
  "description": "An example bot created by @sayanyan to showcase Shoukaku and Indomitable",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "build": "tsup --config tsup-config.json",
    "lint": "eslint --fix --ext .ts",
    "start": "node ./dist/index.js",
    "dev": "node ./dist/index.js | pino-pretty -c -l -t yyyy/mm/dd,HH:MM:ss -i hostname,pid"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deivu/Kongou.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  },
  "keywords": [
    "Discord",
    "Bot",
    "Waifu",
    "music-bot",
    "template-bot",
    "discord.js",
    "discord-js",
    "discordjs",
    "music"
  ],
  "author": "Deivu (Saya)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Deivu/Kongou/issues"
  },
  "homepage": "https://github.com/Deivu/Kongou#readme",
  "dependencies": {
    "@discordjs/rest": "^2.4.3",
    "denque": "^2.1.0",
    "discord-api-types": "^0.37.120",
    "discord.js": "^14.18.0",
    "indomitable": "^4.1.0",
    "pino": "^9.6.0",
    "shoukaku": "github:shipgirlproject/Shoukaku#cd319abde19b23a93d9367a6b28be5077e22e953"
  },
  "devDependencies": {
    "@shipgirl/eslint-config": "^0.4.0",
    "@types/node": "^22.8.2",
    "@types/ws": "^8.5.12",
    "eslint": "^9.25.1",
    "pino-pretty": "^10.3.0",
    "tsup": "^8.4.0",
    "typescript": "^5.6.3"
  }
}
