{
  "name": "kongou",
  "version": "4.0.0",
  "description": "An example bot created by @sayanyan to showcase Shoukaku and Indomitable",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "clean:ts": "tsc --build --clean",
    "build:ts": "tsc --build",
    "build": "npm run clean:ts && npm run build:ts",
    "lint": "eslint --fix --ext .ts",
    "start": "node index.js",
    "dev": "node index.js | pino-pretty -c -l -t yyyy/mm/dd,HH:MM:ss -i hostname,pid"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deivu/Kongou.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  },
  "keywords": [
    "Discord",
    "Bot",
    "Waifu",
    "music-bot",
    "template-bot",
    "discord.js",
    "discord-js",
    "discordjs",
    "music"
  ],
  "author": "Deivu (Saya)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Deivu/Kongou/issues"
  },
  "homepage": "https://github.com/Deivu/Kongou#readme",
  "dependencies": {
    "@discordjs/rest": "^1.7.1",
    "denque": "^2.1.0",
    "discord-api-types": "^0.37.45",
    "discord.js": "^14.11.0",
    "indomitable": "github:Deivu/Indomitable#b6f4ef4",
    "pino": "^8.14.1",
    "shoukaku": "github:Deivu/Shoukaku#31ed644"
  },
  "devDependencies": {
    "@augu/eslint-config": "^4.0.1",
    "@types/node": "^20.3.1",
    "@types/node-fetch": "^2.6.4",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "pino-pretty": "^10.0.0",
    "tsup": "^6.7.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  }
}
